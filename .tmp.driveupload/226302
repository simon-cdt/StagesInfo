                if (flushInMs === 0) {
                    flushNow();
                } else {
                    setTimeout(flushNow, flushInMs);
                }
            }
            setTimeout(runOnNextTick, 0);
        });
    }
    return flushPromiseCache;
}
function flushUpdateQueue({ updateUrl, getSearchParamsSnapshot }) {
    const search = getSearchParamsSnapshot();
    if (updateQueue.size === 0) {
        return [
            search,
            null
        ];
    }
    const items = Array.from(updateQueue.entries());
    const options = {
        ...queueOptions
    };
    const transitions = Array.from(transitionsQueue);
    resetQueue();
    (0, __TURBOPACK__imported__module__$5b$project$5d2f$node_modules$2f$nuqs$2f$dist$2f$chunk$2d$5WWTJYGR$2e$js__$5b$app$2d$ssr$5d$__$28$ecmascript$29$__["debug"])("[nuqs queue] Flushing queue %O with options %O", items, options);
    for (const [key, value] of items){
        if (value === null) {
            search.delete(key);
        } else {
            search.set(key, value);
        }
    }
    try {
        compose(transitions, ()=>{
            updateUrl(search, {
                history: options.history,
                scroll: options.scroll,
                shallow: options.shallow
            });
        });
