function compose(fns, final) {
    const recursiveCompose = (index)=>{
        if (index === fns.length) {
            return final();
        }
        const fn = fns[index];
        if (!fn) {
            throw new Error("Invalid transition function");
        }
        fn(()=>recursiveCompose(index + 1));
    };
    recursiveCompose(0);
}
