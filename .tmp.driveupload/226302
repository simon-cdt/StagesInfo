            search,
            null
        ];
    } catch (err) {
        console.error((0, __TURBOPACK__imported__module__$5b$project$5d2f$node_modules$2f$nuqs$2f$dist$2f$chunk$2d$5WWTJYGR$2e$js__$5b$app$2d$ssr$5d$__$28$ecmascript$29$__["error"])(429), items.map(([key])=>key).join(), err);
