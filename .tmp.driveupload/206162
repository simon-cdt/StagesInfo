        "useFileUpload.useCallback[validateFile]": (file)=>{
            if (file instanceof File) {
                if (file.size > maxSize) {
                    return `Le fichier "${file.name}" dépasse la taille maximum de ${formatBytes(maxSize)}.`;
                }
            } else {
                if (file.size > maxSize) {
                    return `Le fichier "${file.name}" dépasse la taille maximum de ${formatBytes(maxSize)}.`;
                }
            }
            if (accept !== "*") {
                const acceptedTypes = accept.split(",").map({
                    "useFileUpload.useCallback[validateFile].acceptedTypes": (type)=>type.trim()
                }["useFileUpload.useCallback[validateFile].acceptedTypes"]);
                const fileType = file instanceof File ? file.type || "" : file.type;
                const fileExtension = `.${file instanceof File ? file.name.split(".").pop() : file.name.split(".").pop()}`;
                const isAccepted = acceptedTypes.some({
                    "useFileUpload.useCallback[validateFile].isAccepted": (type)=>{
                        if (type.startsWith(".")) {
                            return fileExtension.toLowerCase() === type.toLowerCase();
                        }
                        if (type.endsWith("/*")) {
                            const baseType = type.split("/")[0];
                            return fileType.startsWith(`${baseType}/`);
                        }
                        return fileType === type;
                    }
                }["useFileUpload.useCallback[validateFile].isAccepted"]);
                if (!isAccepted) {
                    return `"${file instanceof File ? file.name : file.name}" a un type de fichier qui n'est pas accepté.`;
                }
            }
            return null;
        }
    }["useFileUpload.useCallback[validateFile]"], [
        accept,
        maxSize
    ]);
