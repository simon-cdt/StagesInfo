        "useFileUpload.useCallback[createPreview]": (file)=>{
            if (file instanceof File) {
                return URL.createObjectURL(file);
            }
            return file.url;
        }
    }["useFileUpload.useCallback[createPreview]"], []);
    const generateUniqueId = (0, __TURBOPACK__imported__module__$5b$project$5d2f$node_modules$2f$next$2f$dist$2f$compiled$2f$react$2f$index$2e$js__$5b$app$2d$client$5d$__$28$ecmascript$29$__["useCallback"])({
        "useFileUpload.useCallback[generateUniqueId]": (file)=>{
            if (file instanceof File) {
                return `${file.name}-${Date.now()}-${Math.random().toString(36).substring(2, 9)}`;
            }
            return file.id;
        }
    }["useFileUpload.useCallback[generateUniqueId]"], []);
