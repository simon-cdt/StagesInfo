        "useFileUpload.useCallback[clearFiles]": ()=>{
            setState({
                "useFileUpload.useCallback[clearFiles]": (prev)=>{
                    // Clean up object URLs
                    prev.files.forEach({
                        "useFileUpload.useCallback[clearFiles]": (file)=>{
                            if (file.preview && file.file instanceof File && file.file.type.startsWith("image/")) {
                                URL.revokeObjectURL(file.preview);
